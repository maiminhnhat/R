<div class="content-wrapper">
    <form action="" method="POST" enctype="multipart/form-data" id="property_form">
        <div class="container-fluid">
            <!-- Breadcrumbs-->
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="home">Dashboard</a>
                </li>
                <li class="breadcrumb-item active">Add listing</li>
            </ol>
            <div class="box_general padding_bottom">
                <div class="header_box version_2">
                    <h2><i class="fa fa-file"></i>Basic info</h2>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <input type="hidden" id="iduser" value="<%=data[0]._id%>">
                            <label>ID</label>
                            <input type="text" class="form-control" placeholder="001" id="property_id" name="property_id" value="<%=data[0].propertyId%>">
                        </div>
                        <div class="form-group">
                            <label> Title</label>
                            <input type="text" class="form-control" placeholder="Hotel Mariott" id="property_title" name="property_title" value="<%=data[0].title%>">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>Category</label>
                            <div class="styled-select">
                                <select id="property_type" name="property_type">
                            <option value="Flat">Flat</option>
                            <option value="Hotel">Hotel</option>
                            <option value="House">House</option>
                            <option value="Unique">Unique</option>
                        </select>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /row-->

                <!-- /row-->
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Description</label>
                            <textarea class="form-control" rows="5" id="property_descript" name="property_descript"><%=data[0].description%></textarea>
                        </div>
                    </div>
                </div>
               
                <!-- /row-->
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Photos</label>

                            <div class="file-loading">

                                <input id="input-pd" name="input-pd[]" type="file" multiple>
                            </div>


                        </div>

                    </div>
                </div>
                <!-- /row-->
            </div>
            <!-- /box_general-->

            <div class="box_general padding_bottom">
                <div class="header_box version_2">
                    <h2><i class="fa fa-map-marker"></i>Location</h2>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Address</label>
                            <input type="text" class="form-control" id="property_address" name="property_address" value="<%=data[0].location.formattedAddress%>">
                        </div>
                    </div>
                </div>
            </div>

            <div class="box_general padding_bottom">
                <div class="header_box version_2">
                    <h2><i class="fa fa-dollar"></i>Pricing</h2>
                </div>
                <div class="row">
                    <div class="col-md-12">

                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Price" id="property_price" name="property_price" value="<%=data[0].price%>">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /row-->
            </div>
            <!-- /box_general-->
            <button type="submit" class="btn btn-primary">Save</button>
        </div>
    </form>

    <!-- /.container-fluid-->
</div>