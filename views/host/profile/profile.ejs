 <!-- /Navigation-->
 <div class="content-wrapper">
    <div class="container-fluid">
      <!-- Breadcrumbs-->
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="#">Dashboard</a>
        </li>
        <li class="breadcrumb-item active">Add listing</li>
      </ol>
		<div class="box_general padding_bottom">
			<div class="header_box version_2">
				<h2><i class="fa fa-user"></i>Profile details</h2>
			</div>
            <form action="" method="POST" id="profile_form">
                <div class="row">	
                    <div class="col-md-12 add_top_30">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Name</label>
                                    <%if(user == null){%>
                                        <input type="text" class="form-control" placeholder="Your name">
                                   <%}else{%>
                                    <input type="text" class="form-control" id="user_name" name="user_name" placeholder="Your name" value="<%=data.name%>">
                                  <%}%>
                                    
                                </div>
                            </div>
                        
                        </div>
                        <!-- /row-->
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Email</label>
                                    <%if(user == null){%>
                                        <input type="email" class="form-control" placeholder="Your email">
                                   <%}else{%>
                                    <input type="email" class="form-control" id="email" name="email" placeholder="Your email" value="<%=data.email%>">
                                  <%}%>
                                    
                                </div>
                            </div>
                        </div>
                        <!-- /row-->
                        <!-- /row-->
                    </div>
                    <button type="submit" class="btn_1 medium">Save</button>
                </div>
            </form>
		
		</div>
		<!-- /box_general-->
        <form action="" method="POST" id="Change_pass_form">
            <div class="row">
                <div class="col-md-12">
                    <div class="box_general padding_bottom">
                        <div class="header_box version_2">
                            <h2><i class="fa fa-lock"></i>Change password</h2>
                        </div>
                        <div class="form-group">
                            <label>New password</label>
                            <input class="form-control" type="password" id="new_password" name="new_password">
                        </div>
                        <div class="form-group">
                            <label>Confirm new password</label>
                            <input class="form-control" type="password" id="re_password" name="re_password">
                        </div>
                        <button type="submit" class="btn_1 medium">Save</button>
                    </div>
                </div>
                
            </div>
        </form>
		
		<!-- /row-->
		
	  </div>
	  <!-- /.container-fluid-->
   	</div>
    <!-- /.container-wrapper-->